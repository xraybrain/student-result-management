{{#if admin_success_msg}}
<div class="col-md-5 mx-auto text-center">
  <p class="alert alert-success">
    <span class="close" data-dismiss="alert">&times;</span>
    {{admin_success_msg}}
  </p>
</div>
{{/if}}

{{#if admin_error_msg}}
<div class="col-md-10 mx-auto text-center">
  <p class="alert alert-success">
    <span class="close" data-dismiss="alert">&times;</span>
    {{admin_error_msg}}
  </p>
</div>
{{/if}}