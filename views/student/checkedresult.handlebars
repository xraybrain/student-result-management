<div class="container col-md-8">
  <div class="card">
    <div class="card-header bg-dark text-light text-center">
      <h3 class="card-title">Checked Result</h3>
    </div>
    <div class="card-body">
      <table class="table table-striped mx-auto col-md-8">
        <thead>
          <tr>
            <th>Course Code</th>
            <th>Grade</th>
          </tr>
        </thead>
        <tbody>
          {{#each resultData}}
          <tr>
            <td>{{courseCode}}</td>
            <td>{{grade}}</td>
          </tr>
          {{else}}

          {{/each}}
        </tbody>
      </table>
    </div>
  </div>

</div>
